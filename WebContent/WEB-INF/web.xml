    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>loginServletName</servlet-name>
    <servlet-class>LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginServletName</servlet-name>
    <url-pattern>/deepak</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>loginSuccessServlet</servlet-name>
    <servlet-class>LoginSuccessServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginSuccessServlet</servlet-name>
    <url-pattern>/success</url-pattern>
  </servlet-mapping>
  
</web-app>